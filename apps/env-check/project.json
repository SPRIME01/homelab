{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "env-check",
  "root": "apps/env-check",
  "sourceRoot": "apps/env-check/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node apps/env-check/src/index.js build"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/tasks/lint-stub.js"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node apps/env-check/tests/smoke.test.js"
      }
    },
    "log-smoke": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/logging/test_node_logger.js"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node apps/env-check/src/index.js deploy"
      },
      "dependsOn": ["build", "log-smoke"]
    }
  },
  "tags": ["type:utility"]
}
